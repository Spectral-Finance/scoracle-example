{
  "name": "scoracle-example",
  "version": "1.0.0",
  "description": "",
  "main": "hardhat.config.ts",
  "scripts": {
    "chain": "hardhat node",
    "chain:fork:arbitrumGoerli": "dotenv cross-var hardhat node --fork https://arb-goerli.g.alchemy.com/v2/%ALCH_STAGING_TOKEN%",
    "console": "hardhat console",
    "console:arbitrumGoerli": "hardhat console --network arbitrumGoerli",
    "compile": "hardhat compile",
    "deploy:myContract:arbitrumGoerli": "hardhat --network arbitrumGoerli run scripts/deployMyContract.script.ts",
    "calculateScore:myContract:arbitrumGoerli": "hardhat --network arbitrumGoerli run scripts/calculateMacroScore.script.ts",
    "prequalifyUser:myContract:arbitrumGoerli": "hardhat --network arbitrumGoerli run scripts/preQualifyUser.script.ts",
    "deploy:myContract:polygon": "hardhat --network polygon run scripts/deployMyContract.script.ts",
    "calculateScore:myContract:polygon": "hardhat --network polygon run scripts/calculateMacroScore.script.ts",
    "prequalifyUser:myContract:polygon": "hardhat --network polygon run scripts/preQualifyUser.script.ts",
    "test": "yarn compile && yarn hardhat test test/*.spec.ts",
    "test:coverage": "yarn compile && yarn hardhat coverage --testfiles 'test/*.spec.ts'"
  },
  "author": "Spectral",
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.3",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/hardhat-upgrades": "^1.22.1",
    "@primitivefi/hardhat-marmite": "0.0.4",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.0.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.4",
    "cross-var": "^1.1.0",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.2",
    "hardhat": "^2.7.0",
    "hardhat-gas-reporter": "^1.0.6",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.19",
    "ts-node": "^10.4.0",
    "typechain": "^6.0.5",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.4.0",
    "@openzeppelin/contracts": "^4.4.1",
    "@openzeppelin/contracts-upgradeable": "^4.4.2"
  }
}
